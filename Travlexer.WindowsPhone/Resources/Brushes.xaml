<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:Imaging="clr-namespace:Codify.Imaging;assembly=Codify"
                    mc:Ignorable="d">
    <SolidColorBrush x:Key="BrushSemiTransparentBackground"
                     Color="#60000000" />
    <SolidColorBrush x:Key="BrushShadow"
                     Color="#FFCCCCCC" />
    <SolidColorBrush x:Key="BrushCaret"
                     Color="{StaticResource PhoneInverseBackgroundColor}" />
    <!--Icon Image Brushes-->
</ResourceDictionary>
